    if (document.querySelector('.alert-success').innerText.includes('Thank') && document.location.pathname == '/contact_us') {
      gtag('event', 'conversion', {
        'send_to': 'AW-16595602617/-HM0CLj8jrsZELmZs-k9'
      });
    }
  });

</script>
    
